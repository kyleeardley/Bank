






<body id = 'terminalbody'>
<div>
    <center>
      <div id = 'displaytitlebox'>
        <h2 id = 'maindisplaytitle'>CLIENT'S INDEX</h2>
      </div>
      <div class = 'currentloansindexmaindisplay'>
          <table class = 'table'>
  			    <% @users.each do |user| %>
    		    
            <tr class = "<%= cycle('listlineodd', 'listlineeven')%>">
              <td class = 'loandata'>
                <dl id = 'borrowername'><h4>Name of Client:&nbsp&nbsp<%=user.username%></h4></dl>
                <dt>Client ID Number:&nbsp&nbsp<%= user.id%></dt>
     			<dt>Email:&nbsp&nbsp<%= user.email%></dt>
     			
                
              </td>
              <td id = 'listactions'>
                <%=link_to 'Show', user%>
                <%=link_to 'Edit', edit_user_path(user)%>
                <%=link_to 'Delete', user_path(user.id), :method => :delete, data: {confirm: 'Are You Sure?'}%>
              </td>
              
            </tr>
            <% end %>
          </table>
        </div> 
      </div>
      
    </center>  
</div>
